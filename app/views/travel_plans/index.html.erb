<p id="notice"><%= notice %></p>

<h1>Travel Plans</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Start schedule at</th>
      <th>End schedule at</th>
      <th>Team</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @travel_plans.each do |travel_plan| %>
      <tr>
        <td><%= travel_plan.name %></td>
        <td><%= travel_plan.start_schedule_at %></td>
        <td><%= travel_plan.end_schedule_at %></td>
        <td><%= travel_plan.team_id %></td>
        <td><%= link_to 'Show', travel_plan %></td>
        <td><%= link_to 'Edit', edit_travel_plan_path(travel_plan) %></td>
        <td><%= link_to 'Destroy', travel_plan, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Travel Plan', new_travel_plan_path %>
